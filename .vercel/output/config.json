{
	"version": 3,
	"routes": [
		{
			"src": "^/_astro/(.*)$",
			"headers": {
				"cache-control": "public, max-age=31536000, immutable"
			},
			"continue": true
		},
		{
			"handle": "filesystem"
		},
		{
			"src": "^\\/_server-islands\\/([^/]+?)\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/_image\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/account\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/api\\/deleteUser\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/blog\\/([^/]+?)\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/blog\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/blogpage1\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/editpost\\/([^/]+?)\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/newpost\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/signup\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/$",
			"dest": "_render"
		}
	]
}